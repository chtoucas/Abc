<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2012/06/nuspec.xsd">
  <!--
    props/targets         -> NuGet 2.5+
    developmentDependency -> NuGet 2.8+
    contentFiles          -> NuGet 3.3+
    buildMultiTargeting   -> NuGet 4.0+
  -->
  <metadata minClientVersion="4.0">
    <!-- Required elements -->
    <id>Abc.Utilities.Sources</id>
    <version>$version$</version>
    <description>
      <![CDATA[
This is a source code only package providing various utilities used internally by the other Abc packages.
    ]]>
    </description>
    <authors>$authors$</authors>

    <!-- Optional elements -->
    <copyright>$copyright$</copyright>
    <developmentDependency>true</developmentDependency>
    <license type="expression">$licenseExpression$</license>
    <projectUrl>$projectUrl$</projectUrl>
    <releaseNotes></releaseNotes>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <repository type="$repositoryType$" url="$repositoryUrl$" />
    <tags>Helpers Utilities</tags>
    <title>Abc Utilities [sources]</title>

    <!--
    <contentFiles>
      <files include="cs/netstandard1.1/**/*.cs" buildAction="Compile" />
    </contentFiles>
    -->
  </metadata>

  <files>
    <file src="readme.txt" target="" />
    <file src="_build\**" target="build\" />
    <file src="System\**\*.cs" target="contentFiles\cs\netstandard1.1\abc.utilities\system\" />
    <file src="**\*.cs" target="contentFiles\cs\netstandard1.1\abc.utilities\" exclude="AssemblyInfo.cs;System\**\*.cs;obj\**" />
  </files>
</package>
